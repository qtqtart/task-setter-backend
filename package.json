{
  "name": "trend-setter-backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "build": "nest build",
    "f:prettier": "prettier --write .",
    "f:eslint": "eslint --fix .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.ts": [
      "pnpm run f:prettier",
      "pnpm run f:eslint"
    ]
  },
  "dependencies": {
    "@nestjs/common": "10.0.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "10.0.0",
    "@nestjs/platform-fastify": "10.0.0",
    "@prisma/client": "^6.1.0",
    "prisma": "^6.1.0",
    "reflect-metadata": "0.2.0",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@eslint/js": "9.17.0",
    "@nestjs/cli": "10.0.0",
    "@nestjs/schematics": "10.0.0",
    "@types/node": "20.3.1",
    "eslint": "9.17.0",
    "eslint-plugin-prettier": "5.2.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "globals": "15.14.0",
    "husky": "9.1.7",
    "lint-staged": "15.2.11",
    "prettier": "3.4.2",
    "source-map-support": "0.5.21",
    "ts-loader": "9.4.3",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.1.3",
    "typescript-eslint": "8.18.2"
  }
}
